{
  "name": "SharePlace",
  "author": "SharePlace",
  "description": "SharePlace",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --content-base dist/ --inline --hot --history-api-fallback=true",
    "build": "NODE_ENV=production webpack -d",
    "open": "electron index.js",
    "pack": "electron-builder",
    "watch": "webpack -d --watch",
    "lint": "eslint './**/*.js'",
    "lintScss": "stylelint 'scss/**/*.scss'"
  },
  "main": "index.js",
  "dependencies": {
    "apollo-client": "^2.2.8",
    "axios": "^0.17.1",
    "babel-cli": "^6",
    "babel-loader": "^7",
    "babel-plugin-module-resolver": "^2",
    "babel-preset-es2015": "^6",
    "babel-preset-es2017": "^6",
    "babel-preset-react": "^6",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-0": "^6",
    "babel-register": "^6",
    "classnames": "^2.2.5",
    "immutability-helper": "^2.6.4",
    "cloudinary-react": "^1.0.6",
    "css-loader": "^0",
    "electron": "^1",
    "electron-builder": "^19",
    "electron-dl": "^1",
    "eslint": "^4",
    "eslint-config-airbnb": "^15",
    "eslint-import-resolver-babel-module": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "file-loader": "^0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2",
    "node-sass": "^4",
    "postcss-loader": "^2",
    "prop-types": "^15",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "react": "^15",
    "react-alert": "^2.4.0",
    "react-apollo": "^2.1.3",
    "graphql": "^0.13.2",
    "lodash": "^4.17.5",
    "react-dom": "^15",
    "react-dropzone": "^4.2.3",
    "react-google-login": "^3.0.4",
    "react-icons": "^2.2.7",
    "react-navigation": "^1.0.3",
    "react-navigation-redux-helpers": "^1.0.1",
    "react-redux": "^5.0.6",
    "react-test-renderer": "16.0.0",
    "redux": "^3.7.2",
    "react-responsive-modal": "^2.0.0",
    "react-splitter-layout": "^3.0.0",
    "react-timezone": "^1.0.5",
    "reactstrap": "^5.0.0-beta.2",
    "sass-loader": "^6",
    "style-loader": "^0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "webpack": "^3",
    "webpack-dev-server": "^2"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "plugins": [
      "import"
    ],
    "settings": {
      "import/resolver": {
        "babel-module": {}
      }
    },
    "rules": {
      "max-len": [
        "error",
        100,
        2,
        {
          "ignoreComments": false,
          "ignoreStrings": false,
          "ignoreTemplateLiterals": false
        }
      ],
      "react/jsx-filename-extension": "off"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "es2017",
      "stage-0",
      "react"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "common": "../common"
          }
        }
      ]
    ]
  },
  "build": {
    "productName": "SharePlace",
    "npmRebuild": false,
    "files": [
      "dist/",
      "index.js",
      "index.html"
    ],
    "directories": {
      "output": "build"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "at-rule-no-unknown": null
    }
  }
}
