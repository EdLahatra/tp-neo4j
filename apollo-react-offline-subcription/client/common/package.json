{
  "private": true,
  "scripts": {
    "lint": "eslint './**/*.js'"
  },
  "dependencies": {
    "apollo-cache-redux": "^0.1.0-alpha.7",
    "apollo-client": "^2.2.5",
    "apollo-link": "^1.1.0",
    "apollo-link-context": "^1.0.5",
    "apollo-link-error": "^1.0.7",
    "apollo-link-http": "^1.3.3",
    "apollo-link-redux": "^0.2.1",
    "apollo-link-ws": "^1.0.5",
    "apollo-utilities": "^1.0.6",
    "axios": "^0.17.0",
    "babel-cli": "^6",
    "babel-loader": "^7",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-es2015": "^6",
    "babel-preset-es2017": "^6",
    "babel-preset-react": "^6",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-0": "^6",
    "babel-register": "^6",
    "base-64": "^0.1.0",
    "eslint": "^4",
    "eslint-config-airbnb": "^15",
    "eslint-import-resolver-babel-module": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.4.2",
    "lodash": "^4.17.5",
    "metro-bundler": "^0.22.1",
    "moment-timezone": "^0.5.14",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-native-fa-icons": "^3.0.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.0",
    "redux-thunk": "^2.2.0",
    "seamless-immutable": "^7.1.3",
    "subscriptions-transport-ws": "^0.9.5"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "plugins": [
      "import"
    ],
    "globals": {
      "WebSocket": true
    },
    "settings": {
      "import/resolver": {
        "babel-module": {}
      }
    },
    "rules": {
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "no-shadow": "off",
      "max-len": [
        "error",
        100,
        2,
        {
          "ignoreComments": false,
          "ignoreStrings": false,
          "ignoreTemplateLiterals": false
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id",
            "_source"
          ]
        }
      ]
    }
  }
}
